{
  "name": "wholesale",
  "version": "1.1.0",
  "description": "Wholesale theme based off Understrap Theme Framework: https://github.com/understrap/understrap",
  "main": "index.js",
  "scripts": {
    "bs": "browser-sync start --config src/build/browser-sync.config.js",
    "css": "npm-run-all css-lint css-compile css-postcss css-minify",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css --quiet src/sass/wholesale.scss:css/wholesale.css src/sass/custom-editor-style.scss:css/custom-editor-style.css",
    "css-lint": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/.stylelintcache --rd",
    "css-minify": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output css/ --batch --batch-suffix \".min\" \"css/*.css\" \"!css/*.min.css\" \"!css/*rtl*.css\"",
    "css-postcss": "postcss --config src/build/postcss.config.js --replace \"css/*.css\" \"!css/*.rtl*.css\" \"!css/*.min.css\"",
    "js": "npm-run-all js-lint js-compile js-minify",
    "js-compile": "rollup --config src/build/rollup.config.js --sourcemap",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives --ext .html,.js .",
    "js-minify": "terser js/wholesale.min.js --config-file src/build/terser.config.json --source-map \"content=js/wholesale.js.map,url=wholesale.min.js.map,filename=wholesale.min.js\" --output js/wholesale.min.js",
    "watch": "npm-run-all --parallel watch-run-*",
    "watch-bs": "npm-run-all --parallel bs watch-run-*",
    "watch-run-css": "nodemon --watch src/sass/ --ext scss --exec \"npm-run-all css\"",
    "watch-run-js": "nodemon --watch src/js/ --ext js --exec \"npm-run-all js\"",
    "copy-assets": "node src/build/copy-assets.js",
    "dist": "npm-run-all --parallel css js",
    "dist-build": "node src/build/dist-build.js",
    "dist-clean": "node src/build/dist-clean.js"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tr1um9h/wholesale.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "framework",
    "bootstrap",
    "underscores"
  ],
  "author": "Nikun Patel",
  "license": "GPL-3.0",
  "homepage": "https://wholesale.loansimple.com",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@popperjs/core": "^2.11.6",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.2",
    "browser-sync": "^2.27.10",
    "browserslist": "^4.21.4",
    "caniuse-lite": "^1.0.30001430",
    "clean-css-cli": "^5.6.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "jquery-match-height": "^0.7.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-understrap-palette-generator": "git+https://github.com/understrap/postcss-understrap-palette-generator.git",
    "prettier": "^2.8.4",
    "rollup": "^3.2.5",
    "sass": "^1.56.0",
    "stylelint": "^14.16.1",
    "stylelint-config-twbs-bootstrap": "^7.0.0",
    "terser": "^5.15.1",
    "understrap": "github:understrap/understrap#develop"
  }
}
